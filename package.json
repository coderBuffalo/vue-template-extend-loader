{
    "name": "vue-template-extend-loader",
    "version": "0.1.1",
    "description": "vue template loader for extending",
    "author": "johnsonwu",
    "private": true,
    "main": "src/index.js",
    "scripts": {
        "build": "webpack --target node",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "license": "MIT",
    "peerDependencies": {
        "cheerio": "^1.0.0-rc.3"
    },
    "dependencies": {
        "cheerio": "^1.0.0-rc.3"
    },
    "devDependencies": {
        "loader-utils": "^1.2.3",
        "schema-utils": "^2.6.1"
    },
    "schemaOptions": {
        "type": "object",
        "additionalProperties": false,
        "definitions": {
            "noEmptyStr": {
                "type": "string",
                "minLength": 2
            }
        },
        "properties": {
            "tag_template": {
                "$ref": "#/definitions/noEmptyStr"
            },
            "tag_extender": {
                "$ref": "#/definitions/noEmptyStr"
            },
            "tag_extends": {
                "$ref": "#/definitions/noEmptyStr"
            },
            "extender_prop_path": {
                "$ref": "#/definitions/noEmptyStr"
            },
            "extends_prop_query": {
                "$ref": "#/definitions/noEmptyStr"
            },
            "extends_prop_mode": {
                "$ref": "#/definitions/noEmptyStr"
            },
            "extends_prop_mode_default": {
                "$ref": "#/definitions/noEmptyStr"
            },
            "mode": {
                "type": "object",
                "additionalProperties": true
            },
            "cheerioOptions": {
                "type": "object",
                "additionalProperties": true
            }
        }
    },
    "extenderOptions": {
        "tag_template": "template",
        "tag_extender": "gacExtender",
        "tag_extends": "gacExtend",
        "extender_prop_path": "path",
        "extends_prop_query": "query",
        "extends_prop_mode": "mode",
        "extends_prop_mode_default": "html",
        "mode": {
            "append": "append",
            "prepend": "prepend",
            "after": "after",
            "before": "before",
            "html": "html",
            "wrap": "wrap",
            "wrapInner": "wrapInner",
            "replace": "replaceWith",
            "remove": "remove"
        },
        "cheerioOptions": {
            "xmlMode": true,
            "decodeEntities": false
        }
    }
}
